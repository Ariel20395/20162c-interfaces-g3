<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ui.grupo3.rankit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CalificacionListActivity"
            android:label="@string/title_calificacion_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".activity.CalificacionDetailActivity"
            android:label="@string/title_calificacion_detail"
            android:parentActivityName=".activity.CalificacionListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ui.grupo3.rankit.activity.CalificacionListActivity" />
        </activity>
    </application>

</manifest>